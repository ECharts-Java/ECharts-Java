{
  "name": "GraphSeriesOption",
  "type": "interface",
  "extends": [
    "SeriesOption",
    "SeriesOnCartesianOptionMixin",
    "SeriesOnPolarOptionMixin",
    "SeriesOnCalendarOptionMixin",
    "SeriesOnGeoOptionMixin",
    "SeriesOnSingleOptionMixin",
    "SymbolOptionMixin",
    "RoamOptionMixin",
    "BoxLayoutOptionMixin"
  ],
  "fields": [
    { "name": "type", "types": ["String"], "default": "graph" },
    { "name": "coordinateSystem", "types": ["String"] },
    { "name": "legendHoverLink", "types": ["Boolean"] },
    { "name": "layout", "types": ["String"] },
    {
      "name": "data",
      "types": [
        "String[]",
        "String[][]",
        "Number[]",
        "Number[][]",
        "Date[]",
        "Date[][]",
        "GraphNodeItemOption[]"
      ]
    },
    {
      "name": "nodes",
      "types": [
        "String[]",
        "String[][]",
        "Number[]",
        "Number[][]",
        "Date[]",
        "Date[][]",
        "GraphNodeItemOption[]"
      ]
    },
    { "name": "edges", "types": ["GraphEdgeItemOption[]"] },
    { "name": "links", "types": ["GraphEdgeItemOption[]"] },
    { "name": "categories", "types": ["GraphCategoryItemOption[]"] },
    { "name": "focusNodeAdjacency", "types": ["Boolean"] },
    { "name": "nodeScaleRatio", "types": ["Number"] },
    { "name": "draggable", "types": ["Boolean"] },
    { "name": "edgeSymbol", "types": ["String[]", "String"] },
    { "name": "edgeSymbolSize", "types": ["Number", "Number[]"] },
    { "name": "edgeLabel", "types": ["SeriesLineLabelOption"] },
    { "name": "label", "types": ["SeriesLabelOption"] },
    { "name": "itemStyle", "types": ["ItemStyleOption"] },
    { "name": "lineStyle", "types": ["GraphEdgeLineStyleOption"] },
    { "name": "emphasis", "types": ["GraphEmphasisOption"] },
    { "name": "blur", "types": ["GraphBlurOption"] },
    { "name": "select", "types": ["GraphSelectOption"] },
    { "name": "circular", "types": ["GraphCircularOption"] },
    { "name": "force", "types": ["GraphForceOption"] }
  ],
  "comments": [
    "https://github.com/apache/echarts/blob/566ba8e83dd283ba359fe9aceee0d4a12dbcd6c7/src/chart/graph/GraphSeries.ts#L139"
  ]
}
